              <button class="tablinks btn btn-dark" onclick="openTab(event, 'floatframe')">FLOAT</button>
            </p>

            <br>
            <br>
            <div id="whiteframe" class="tabcontent">
              <p>Select frames as per your choice!</p>


              <div id="scrollable">

                <div id="frame_items">
                  <%
                if(whiteframe.length!=0){
                var i=1;
                whiteframe.forEach(function(data){
                %>
                  <div class="item" style="padding:10px;">

                    <div class="card" style="width: 18rem; background:#f1f0f0; padding: 5%; text-align: center;"
                      id="cardi">

                      <img class="card-img-top " src="<%=data.FRTHIMG %>" alt="Card image cap"
                        data-src="<%=data.FRIMG %>" style="width: 30px;" id="<%=data.FRID %>">
                      <div class="card-body" style="text-align: center;">
                        <h5 class="card-title">CODE :<%=data.FRCODE %></h5>
                        <p class="card-text">

                        <ul style="text-align: start;">
                          <li>
                            Width : <%=data.FRWIDTH %> cm
                          </li>
                          <li>
                            Depth : <%=data.FRDEPTH %> cm
                          </li>
                          <li>
                            Rebate : <%=data.FRREBATE %> cm
                          </li>
                          <li>
                            Material : <%=data.FRMATERIAL %>
                          </li>
                          <li>
                            Color :<%=data.FRCOLOR %>
                          </li>
                          <li>
                            Price Rate : <%=data.FRCOST %>
                          </li>

                        </ul>
                        </p>


                      </div>
                    </div>
                  </div>
                  <%  i++; }) %>
                  <% } else{ %>
                  <tr>
                    <td colspan="7">No Data Found</td>
                  </tr>
                  <% } %>



                </div>
              </div>
